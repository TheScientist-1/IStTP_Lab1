@model IEnumerable<GalleryWebApplication.Models.User>

<h2>List of users</h2>
<table class="table">
    @foreach (var user in Model)
    {
            <tr>
                <td>@user.Email</td>
                    <td>@user.Name</td>
                <td>
                        <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Edit access rights</a>
                </td>
            </tr>
    }
</table>

